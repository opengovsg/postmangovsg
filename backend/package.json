{
  "name": "backend",
  "version": "1.35.0",
  "description": "Backend / API server for Postman",
  "main": "build/server.js",
  "scripts": {
    "build": "rimraf build && tsc --build tsconfig.build.json",
    "dev": "npm run postbuild && tsc-watch --onSuccess \"node ./build/server.js\"",
    "lint-no-fix": "tsc --noEmit && eslint --ext .js,.ts --cache .",
    "lint": "npm run lint-no-fix -- --fix",
    "postbuild": "npm run copy-assets",
    "start": "node build/server",
    "copy-assets": "copyfiles -u 1 src/assets/* src/**/*.sql build",
    "pretest": "tsc --build",
    "test": "jest --runInBand --detectOpenHandles",
    "precommit": "lint-staged",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:undo": "npx sequelize db:migrate:undo"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sns": "3.7.0",
    "@sentry/node": "5.16.1",
    "async-retry": "1.3.3",
    "aws-sdk": "2.884.0",
    "axios": "0.21.4",
    "bcrypt": "5.0.0",
    "bee-queue": "1.4.0",
    "celebrate": "12.0.1",
    "connect-redis": "4.0.4",
    "convict": "6.2.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "escape-html": "1.0.3",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "express-session": "1.17.0",
    "express-winston": "4.2.0",
    "helmet": "3.23.3",
    "jsonwebtoken": "8.5.1",
    "libphonenumber-js": "1.9.16",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "nodemailer": "6.6.2",
    "otplib": "12.0.1",
    "papaparse": "5.2.0",
    "pg": "8.5.1",
    "pg-connection-string": "2.4.0",
    "rate-limit-redis": "2.1.0",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.17.0",
    "sequelize-typescript": "2.1.3",
    "source-map-support": "0.5.19",
    "starkbank-ecdsa": "1.1.4",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "4.4.0",
    "telegraf": "3.38.0",
    "threads": "1.4.0",
    "tiny-worker": "2.3.0",
    "twilio": "3.58.0",
    "uuid": "7.0.3",
    "validator": "13.7.0",
    "winston": "3.3.3",
    "winston-cloudwatch": "3.1.1"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.4",
    "@types/bcrypt": "3.0.0",
    "@types/bluebird": "3.5.30",
    "@types/cheerio": "0.22.31",
    "@types/connect-redis": "0.0.13",
    "@types/convict": "5.2.1",
    "@types/cors": "2.8.6",
    "@types/escape-html": "1.0.0",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "5.1.1",
    "@types/express-session": "1.17.0",
    "@types/hapi__joi": "16.0.12",
    "@types/helmet": "0.0.45",
