{
  "name": "frontend",
  "version": "1.22.4",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm start",
    "build": "npm run compile && INLINE_RUNTIME_CHUNK=false react-scripts build",
    "eject": "react-scripts eject",
    "lint-no-fix": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx --cache . && prettier --check '**/*.{md,scss}'",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx --cache --fix . && prettier --write '**/*.{md,scss}' --loglevel warn",
    "precommit": "lint-staged",
    "get-sentry-release": "sentry-cli releases propose-version",
    "upload-source-map": "node sentry.js",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "test": "react-scripts test --passWithNoTests"
  },
  "dependencies": {
    "@lingui/react": "^3.5.1",
    "@sentry/browser": "^5.27.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/papaparse": "^5.2.5",
    "@types/validator": "^13.0.0",
    "axios": "^0.21.1",
    "boxicons": "^2.0.5",
    "buffer": "^5.6.0",
    "classnames": "^2.2.6",
    "downloadjs": "^1.4.7",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "escape-html": "^1.0.3",
    "html-to-draftjs": "^1.5.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.1.0",
    "moment": "^2.29.1",
    "papaparse": "^5.2.0",
    "postman-templating": "file:../modules/postman-templating",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.6",
    "react-ga": "^2.7.0",
    "react-moment": "^0.9.7",
    "react-paginate": "^6.3.2",
    "react-player": "^2.6.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "react-textarea-autosize": "^7.1.2",
    "sass-burger": "^1.3.1",
    "spark-md5": "^3.0.1",
    "typescript": "^4.2.3",
    "uuidv4": "^6.1.1",
    "validator": "^13.6.0",
    "webcrypto-shim": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@lingui/cli": "^3.5.1",
    "@lingui/macro": "^3.5.1",
    "@peculiar/webcrypto": "^1.1.6",
    "@sentry/cli": "^1.54.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.1",
    "@types/classnames": "^2.2.10",
    "@types/downloadjs": "^1.4.2",
    "@types/draft-js": "^0.10.44",
    "@types/draftjs-to-html": "^0.8.0",
    "@types/escape-html": "0.0.20",
    "@types/html-to-draftjs": "^1.4.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-draft-wysiwyg": "^1.13.0",
    "@types/react-paginate": "^6.2.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-textarea-autosize": "^4.3.5",
    "@types/spark-md5": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^10.2.6",
    "msw": "^0.27.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{md,scss}": "prettier --write"
  }
}
