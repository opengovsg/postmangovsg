{
  "name": "frontend",
  "private": true,
  "proxy": "https://api-staging.postman.gov.sg",
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "npm start",
    "prebuild": "npm run compile && tsc --build",
    "build": "INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "eject": "react-scripts eject",
    "lint-no-fix": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx --cache . && prettier --check '**/*.{md,scss}'",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx --cache --fix . && prettier --write '**/*.{md,scss}' --loglevel warn",
    "precommit": "lint-staged",
    "get-sentry-release": "sentry-cli releases propose-version",
    "upload-source-map": "node sentry.js",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "pretest": "tsc --build",
    "test": "react-app-rewired test --passWithNoTests"
  },
  "dependencies": {
    "@datadog/browser-rum": "4.43.0",
    "@lingui/core": "3.14.0",
    "@lingui/react": "3.14.0",
    "@sentry/browser": "5.27.6",
    "@types/bcryptjs": "2.4.2",
    "@types/papaparse": "5.2.5",
    "@types/validator": "13.0.0",
    "axios": "1.6.0",
    "boxicons": "2.0.5",
    "buffer": "5.7.1",
    "classnames": "2.2.6",
    "downloadjs": "1.4.7",
    "draft-js": "0.11.7",
    "draft-js-import-html": "^1.4.1",
    "draftjs-filters": "^3.0.1",
    "draftjs-to-html": "0.9.1",
    "escape-html": "1.0.3",
    "html-to-draftjs": "1.5.0",
    "immutable": "3.8.2",
    "lodash": "4.17.21",
    "lottie-react": "2.3.0",
    "make-plural": "6.2.2",
    "moment": "2.29.4",
    "papaparse": "5.2.0",
    "querystring": "0.2.1",
    "react": "18.1.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.1.0",
    "react-draft-wysiwyg": "1.15.0",
    "react-dropzone": "14.2.3",
    "react-ga": "3.3.1",
    "react-moment": "1.1.2",
    "react-paginate": "8.1.3",
    "react-player": "2.6.2",
    "react-router-dom": "6.3.0",
    "react-textarea-autosize": "8.3.4",
    "sass-burger": "1.3.1",
    "sms-segments-calculator": "1.1.1",
    "spark-md5": "3.0.1",
    "stream": "0.0.2",
    "typescript": "4.7.4",
    "url": "0.11.0",
    "util": "0.12.4",
    "uuidv4": "6.1.1",
    "validator": "13.7.0",
    "webcrypto-shim": "0.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@lingui/cli": "3.14.0",
    "@lingui/macro": "3.14.0",
    "@peculiar/webcrypto": "1.1.6",
    "@sentry/cli": "1.64.2",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.1.0",
    "@types/classnames": "2.2.10",
    "@types/downloadjs": "1.4.2",
    "@types/draft-js": "0.10.44",
    "@types/draftjs-to-html": "0.8.0",
    "@types/escape-html": "0.0.20",
    "@types/html-to-draftjs": "1.4.0",
    "@types/jest": "26.0.21",
    "@types/lodash": "4.14.165",
    "@types/node": "12.12.34",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/react-draft-wysiwyg": "1.13.4",
    "@types/react-paginate": "6.2.1",
    "@types/react-router-dom": "5.1.3",
    "@types/react-textarea-autosize": "4.3.5",
    "@types/spark-md5": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "jest-canvas-mock": "2.5.2",
    "lint-staged": "10.2.6",
