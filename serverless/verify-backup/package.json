    "@google-cloud/kms": "2.11.1",
    "@google-cloud/secret-manager": "3.12.0",
    "@google-cloud/storage": "5.20.5",
    "@sentry/cli": "1.58.0",
    "@types/express": "4.17.11",
    "body-parser": "1.19.0",
    "convict": "6.2.3",
    "dotenv": "8.6.0",
    "express": "4.17.1",
    "lodash": "4.17.21"
  },
