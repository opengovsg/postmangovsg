{
  "name": "antivirus-scanner",
  "version": "1.0.0",
  "description": "Antivirus scanner for Postman",
  "scripts": {
    "env": "ENV=${ENV:-'development'} node scripts/envLoader.mjs",
    "build": "tsc",
    "tsc": "tsc",
    "lint-no-fix": "tsc --noEmit && eslint --ext .js,.ts --cache . --ignore-pattern 'cdk/*'",
    "lint": "npm run lint-no-fix -- --fix",
    "dev": "npm run env && docker-compose -f docker-compose.dev.yml up --build",
    "start": "node build/index"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.131.0",
    "@typescript-eslint/parser": "^5.36.2",
    "axios": "^0.27.2",
    "convict": "^6.2.3",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/node": "^18.7.13",
    "@types/watch": "^1.0.3",
    "dotenv-cli": "^6.0.0",
    "typescript": "^4.7.4"
  }
}
